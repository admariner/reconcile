{
    "name": "reconcile",
    "description": "Add new columns to your spreadsheet based on looking up names or numbers.",
    "version": "1.3.0",
    "author": "Max Harlow <maxharlow@gmail.com> (http://maxharlow.com/)",
    "license": "Apache-2.0",
    "preferGlobal": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/maxharlow/reconcile.git"
    },
    "dependencies": {
        "yargs": "4.7.x",
        "highland": "2.8.x",
        "request": "2.72.x",
        "js-yaml": "3.6.x",
        "csv-parser": "1.9.x",
        "csv-write-stream": "2.0.x"
    },
    "bin": {
        "reconcile": "./reconcile"
    },
    "scripts": {
        "postinstall": "(reconcile completion > /etc/bash_completion.d/reconcile || reconcile completion > /usr/local/etc/bash_completion.d/reconcile || true) 2> /dev/null",
        "postuninstall": "(rm /etc/bash_completion.d/reconcile || rm /usr/local/etc/bash_completion.d/reconcile || true) 2> /dev/null"
    }
}
